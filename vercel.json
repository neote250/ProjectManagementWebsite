{
    "version": 2,
    "builds": [
        {
            "src": "Frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        },
        {
            "src": "Backend2/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Frontend/build/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/Backend2/$1"
        }
    ]
}