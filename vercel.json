{
    "version": 2,
    "builds": [
        {
            "src": "Frontend/package.json",
            "use": "@vercel/vite",
            "config": {
                "distDir": "Frontend/build"
            }
        },
        {
            "src": "Backend2/Server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/Frontend/build/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/Backend2/Server.js"
        }
    ]
}